<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>HelloVue3Cordova</name>
  <description>
        A sample Vue3 Cordova application that responds to the deviceready event.
    </description>
  <author email="dev@dev.org" href="http://cordova.io">
        Marco Chierchia
    </author>
  <content src="http://192.168.1.9:8080"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <plugin name="@appcominteractive/cordova-plugin-hot-reload">
    <variable name="publicPath" value="www"/>
  </plugin>
  <allow-navigation href="http://192.168.1.9:8080/*"/>
</widget>